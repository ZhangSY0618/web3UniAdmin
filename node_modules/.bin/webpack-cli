#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/mnt/d/yideng32/web3UniAdmin/node_modules/.pnpm/webpack-cli@5.1.4_webpack-b_d9d1b043c3315f5e196aeba5ab2f9a9f/node_modules/webpack-cli/bin/node_modules:/mnt/d/yideng32/web3UniAdmin/node_modules/.pnpm/webpack-cli@5.1.4_webpack-b_d9d1b043c3315f5e196aeba5ab2f9a9f/node_modules/webpack-cli/node_modules:/mnt/d/yideng32/web3UniAdmin/node_modules/.pnpm/webpack-cli@5.1.4_webpack-b_d9d1b043c3315f5e196aeba5ab2f9a9f/node_modules:/mnt/d/yideng32/web3UniAdmin/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/mnt/d/yideng32/web3UniAdmin/node_modules/.pnpm/webpack-cli@5.1.4_webpack-b_d9d1b043c3315f5e196aeba5ab2f9a9f/node_modules/webpack-cli/bin/node_modules:/mnt/d/yideng32/web3UniAdmin/node_modules/.pnpm/webpack-cli@5.1.4_webpack-b_d9d1b043c3315f5e196aeba5ab2f9a9f/node_modules/webpack-cli/node_modules:/mnt/d/yideng32/web3UniAdmin/node_modules/.pnpm/webpack-cli@5.1.4_webpack-b_d9d1b043c3315f5e196aeba5ab2f9a9f/node_modules:/mnt/d/yideng32/web3UniAdmin/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../webpack-cli/bin/cli.js" "$@"
else
  exec node  "$basedir/../webpack-cli/bin/cli.js" "$@"
fi
